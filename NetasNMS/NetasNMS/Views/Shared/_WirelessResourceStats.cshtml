<script type="text/javascript">
    var PieCharts = function () {
        return {
            initEasyPieCharts: function () {
                if (!jQuery().easyPieChart) {
                    return;
                }

                $('.easy-pie-chart .number.transactions.aconline').easyPieChart({
                    animate: 1000,
                    size: 120,
                    lineWidth: 8,
                    barColor: App.getBrandColor('green')
                });

                $('.easy-pie-chart .number.transactions.acoffline').easyPieChart({
                    animate: 1000,
                    size: 120,
                    lineWidth: 8,
                    barColor: App.getBrandColor('red')
                });

                $('.easy-pie-chart .number.transactions.apmanaged').easyPieChart({
                    animate: 1000,
                    size: 120,
                    lineWidth: 8,
                    barColor: App.getBrandColor('blue')
                });

                $('.easy-pie-chart .number.transactions.apconfail').easyPieChart({
                    animate: 1000,
                    size: 120,
                    lineWidth: 8,
                    barColor: App.getBrandColor('red')
                });
            }
        }
    }();

    $(document).ready(function () {

        var AcStatusViewModel = {
            Online: ko.observable(),
            Offline: ko.observable()
        }
        ko.mapping.fromJS({ 'Online': 1100, 'Offline': 100 }, {}, AcStatusViewModel);
        AcStatusViewModel.total = ko.computed(function () { return this.Online() + this.Offline() }, AcStatusViewModel);
        AcStatusViewModel.onlinepercentage = ko.computed(function () { return (this.Online() / (this.Online() + this.Offline())) * 100 }, AcStatusViewModel);
        AcStatusViewModel.offlinepercentage = ko.computed(function () { return (this.Offline() / (this.Online() + this.Offline())) * 100 }, AcStatusViewModel);

        ko.applyBindings(AcStatusViewModel, document.getElementById('acstatus'));

        PieCharts.initEasyPieCharts(); // init metronic core componets
    });
</script>