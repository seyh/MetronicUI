<script src="../assets/global/plugins/jquery.min.js" type="text/javascript"></script>
<script type="text/javascript">
    var PieCharts = function () {
        return {
            initEasyPieCharts: function () {
                if (!jQuery().easyPieChart) {
                    return;
                }

                $('.easy-pie-chart .number.transactions.aconline').easyPieChart({
                    animate: 1000,
                    size: 120,
                    lineWidth: 8,
                    barColor: App.getBrandColor('green')
                });

                $('.easy-pie-chart .number.transactions.acoffline').easyPieChart({
                    animate: 1000,
                    size: 120,
                    lineWidth: 8,
                    barColor: App.getBrandColor('red')
                });

                $('.easy-pie-chart .number.transactions.apmanaged').easyPieChart({
                    animate: 1000,
                    size: 120,
                    lineWidth: 8,
                    barColor: App.getBrandColor('blue')
                });

                $('.easy-pie-chart .number.transactions.apconfail').easyPieChart({
                    animate: 1000,
                    size: 120,
                    lineWidth: 8,
                    barColor: App.getBrandColor('red')
                });
            }
        }
    }();

    $(document).ready(function () {

        //AC Part
        var AcStatusViewModel = {
            Online: ko.observable(),
            Offline: ko.observable()
        }
        ko.mapping.fromJS({ 'Online': 1100, 'Offline': 100 }, {}, AcStatusViewModel);
        if (!AcStatusViewModel.Online()) AcStatusViewModel.Online(0);
        if (!AcStatusViewModel.Offline()) AcStatusViewModel.Offline(0);

        AcStatusViewModel.Total = ko.computed(function () { return this.Online() + this.Offline() }, AcStatusViewModel);
        AcStatusViewModel.OnlinePercentage = ko.computed(function () { return (this.Online() / (this.Online() + this.Offline())) * 100 }, AcStatusViewModel);
        AcStatusViewModel.OfflinePercentage = ko.computed(function () { return (this.Offline() / (this.Online() + this.Offline())) * 100 }, AcStatusViewModel);
        ko.applyBindings(AcStatusViewModel, document.getElementById('acstatus'));

        //AP Part
        var ApStatusViewModel = {
            Managed: ko.observable(),
            Connection_Failed: ko.observable()
        }
        ko.mapping.fromJS({ 'Managed': 42000, 'Connection_Failed': 100 }, {}, ApStatusViewModel);
        if (!ApStatusViewModel.Managed()) ApStatusViewModel.Managed(0);
        if (!ApStatusViewModel.Connection_Failed()) ApStatusViewModel.Connection_Failed(0);

        ApStatusViewModel.Total = ko.computed(function () { return this.Managed() + this.Connection_Failed() }, ApStatusViewModel);
        ApStatusViewModel.ManagedPercentage = ko.computed(function () { return (this.Managed() / (this.Managed() + this.Connection_Failed())) * 100 }, ApStatusViewModel);
        ApStatusViewModel.ConnectionFailedPercentage = ko.computed(function () { return (this.Connection_Failed() / (this.Managed() + this.Connection_Failed())) * 100 }, ApStatusViewModel);
        ko.applyBindings(ApStatusViewModel, document.getElementById('apstatus'));

        PieCharts.initEasyPieCharts();
    });
</script>