
@{
    ViewBag.Title = "Acknowledge Event Alarms Report";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- BEGIN CONTENT -->
<div class="page-content-wrapper">
    <!-- BEGIN CONTENT BODY -->
    <div class="page-content">
        <!-- BEGIN PAGE HEADER-->
        <h1 class="page-title">
            Reporting
        </h1>
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li>
                    <i class="icon-home"></i>
                    <a href="/">Home</a>
                    <i class="fa fa-angle-right"></i>
                </li>
                <li>
                    <span>Acknowledge Alarms Report</span>
                </li>
            </ul>
            <div class="page-toolbar">
                <div class="btn-group pull-right">
                    <span type="button" class="btn btn-fit-height grey-salt dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-delay="1000" data-close-others="true">
                        Actions
                    </span>
                </div>
            </div>
        </div>
        <!-- END PAGE HEADER-->
        <div class="m-heading-1 border-green m-bordered">
            <h3 style="margin-top:0px; padding-left:20px; width:100%; border-bottom:solid 1px #eef1f5; padding-bottom:10px;">Report Action</h3>
            <div class="portlet-body form">
                <!-- BEGIN FORM-->
                <form action="#" class="form-horizontal">
                    <div class="form-body">
                        <div class="form-group">
                            <div class="col-md-3">
                                <div class="input-group date form_date form_datetime bs-datetime" data-date="" data-date-format="dd MM yyyy" data-link-field="dtp_input2" data-link-format="yyyy-mm-dd">
                                    <input class="form-control" size="16" type="text" value="Select start date" readonly>
                                    <span class="input-group-addon">
                                        <button class="btn default date-reset" type="button">
                                            <i class="fa fa-times"></i>
                                        </button>
                                    </span>
                                    <span class="input-group-addon">
                                        <button class="btn default date-set" type="button">
                                            <i class="fa fa-calendar"></i>
                                        </button>
                                    </span>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="input-group date form_date form_datetime bs-datetime" data-date="" data-date-format="dd MM yyyy" data-link-field="dtp_input2" data-link-format="yyyy-mm-dd">
                                    <input class="form-control" size="16" type="text" value="Select end date" readonly>
                                    <span class="input-group-addon">
                                        <button class="btn default date-reset" type="button">
                                            <i class="fa fa-times"></i>
                                        </button>
                                    </span>
                                    <span class="input-group-addon">
                                        <button class="btn default date-set" type="button">
                                            <i class="fa fa-calendar"></i>
                                        </button>
                                    </span>
                                </div>
                            </div>
                            <div class="col-md-3">
                            </div>
                            <div class="col-md-3">
                                <div class="table-group-actions pull-right">
                                    <span> </span>
                                    <div class="btn-group">
                                        <button class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="true">
                                            Filter Table
                                            <i class="fa fa-angle-down"></i>
                                        </button>
                                        <ul class="dropdown-menu" role="menu">
                                            <li role="presentation">
                                                <a role="menuitem" tabindex="-1" href="javascript:;">
                                                    Action
                                                    <span class="badge badge-success"> 2 </span>
                                                </a>
                                            </li>
                                            <li role="presentation">
                                                <a role="menuitem" tabindex="-1" href="javascript:;">
                                                    Another action
                                                    <span class="badge badge-warning"> 5 </span>
                                                </a>
                                            </li>
                                            <li role="presentation">
                                                <a role="menuitem" tabindex="-1" href="javascript:;">
                                                    Something here
                                                    <span class="badge badge-danger"> 7 </span>
                                                </a>
                                            </li>
                                            <li role="presentation" class="divider"> </li>
                                            <li role="presentation">
                                                <a role="menuitem" tabindex="-1" href="javascript:;">
                                                    Separated link
                                                    <span class="badge badge-info"> 12 </span>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                    <button class="btn green table-group-action-submit">
                                        <i class="fa fa-check"></i> Submit
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<!-- END CONTENT -->

